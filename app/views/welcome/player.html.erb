<a href="#" id="enter-song"><h1>Add a song</h1></a>
<a href="#" id="choose-mood"><h1>Choose a mood</h1></a>

<div id="mood-selection">
  <h1>How are you feeling?</h1>
  <button class="emotion" id="happy"> <span> Happy </span>  </button>
  <button class="emotion" id="melancholy">Melancholy</button>
  <button class="emotion" id="excited">Excited</button>
  <button class="emotion" id="chill">Chill</button>
  <button class="emotion" id="angry">Angry</button>
</div>

<div id="slides" >
  <div id="slide1">
    <a href="#" id="hide">hide</a>
    <h1>FIRST SLIDE TEST</h1>
    <a href="#" id="submit">Submit form</a>
  </div>

  <div class="" id="slide2">
    <h1>SECOND SLIDE TEST</h1>
    <a href="#" id="next-slide">click for next slide</a>
  </div>

  <div id="slide3">
    <h1>THIRD SLIDE TEST</h1>
    <a href="#" id="finish">finish</a>
  </div>
</div>

<div id="three"></div>

<div id="playlist">

  <h3><marquee behavior="scroll" direction="left">Track playing will be displayed here</marquee></h3>

  <audio id="audio" controls="controls">
      <source id="mp3Source" type="audio/mp3"></source>
      Your browser does not support the audio format.
  </audio>

  <% if current_user %>
    <img src = '/assets/soundcloud.png' onclick="location.href = '/soundcloud-disconnect'" id = "soundcloud" title="Disconnect from Soundcloud">
  <% else %>
    <img src = '/assets/soundcloud.png' onclick="location.href = '/soundcloud-callback'" id= "soundcloud" title="Connect to Soundcloud">
  <% end %>

<div>
  <input type="text">
  <button id = "go">Search</button>
</div>

<div id = 'songList'>
</div>

<div>
  <div id = 'moodDropdown'>
  </div>
  <div id = 'submit'>
    <button id='ajax'>Enter</button>
  </div>
</div>
